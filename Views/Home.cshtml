.c1 {
    /*Square Div*/
		/*width: 100px;
    	height: 100px;
    	background-color: red;*/
	/*Circle Div*/
	border-radius: 50%;
	width: 24%;
	height: auto;
	padding-top: 24%;
	background: #ef8913;
    position: relative;
    -webkit-animation-name: c1; /* Safari 4.0 - 8.0 */
    -webkit-animation-duration: 4s; /* Safari 4.0 - 8.0 */
    -webkit-animation-iteration-count: infinite; /* Safari 4.0 - 8.0 */
    animation-name: c1;
    animation-duration: 4s;
    animation-iteration-count: infinite;
}

/* Safari 4.0 - 8.0 */
@-webkit-keyframes c1 {
    0%   {background-color:red; left:0px; top:0px;}
    25%  {background-color:yellow; left:900px; top:0px;}
    50%  {background-color:blue; left:200px; top:200px;}
    75%  {background-color:green; left:750px; top:400px;}
    100% {background-color:red; left:0px; top575px;}
}

/* Standard syntax */
@keyframes c1 {
    0%   {background-color:red; left:0px; top:0px;}
    25%  {background-color:yellow; left:900px; top:0px;}
    50%  {background-color:blue; left:200px; top:200px;}
    75%  {background-color:green; left:750px; top:400px;}
    100% {background-color:red; left:0px; top:575px;}
}
.c2 {
    /*Square Div*/
		/*width: 100px;
    	height: 100px;
    	background-color: red;*/
	/*Circle Div*/
	border-radius: 50%;
	width: 12%;
	height: auto;
	padding-top: 12%;
	background: #A9B95C;
    position: relative;
    -webkit-animation-name: c2; /* Safari 4.0 - 8.0 */
    -webkit-animation-duration: 4s; /* Safari 4.0 - 8.0 */
    -webkit-animation-iteration-count: infinite; /* Safari 4.0 - 8.0 */
    animation-name: c2;
    animation-duration: 4s;
    animation-iteration-count: infinite;
}

/* Safari 4.0 - 8.0 */
@-webkit-keyframes c2 {
    0%   {background-color:red; left:500px; top:0px;}
    25%  {background-color:yellow; left:-10px; top:200px;}
    50%  {background-color:blue; left:700px; top:-50px;}
    75%  {background-color:green; left:30px; top:-80px;}
    100% {background-color:red; left:320px; top:-100px;}
}

/* Standard syntax */
@keyframes c2 {
    0%   {background-color:red; left:500px; top:0px;}
    25%  {background-color:yellow; left:-10px; top:200px;}
    50%  {background-color:blue; left:700px; top:-50px;}
    75%  {background-color:green; left:30px; top:-80px;}
    100% {background-color:red; left:320px; top:-100px;}
}
.c3 {
    /*Square Div*/
		/*width: 100px;
    	height: 100px;
    	background-color: red;*/
	/*Circle Div*/
	border-radius: 50%;
	width: 4%;
	height: auto;
	padding-top: 4%;
