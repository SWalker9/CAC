    -webkit-animation-name: c3; /* Safari 4.0 - 8.0 */
    -webkit-animation-duration: 4s; /* Safari 4.0 - 8.0 */
    -webkit-animation-iteration-count: infinite; /* Safari 4.0 - 8.0 */
    animation-name: c3;
    animation-duration: 4s;
    animation-iteration-count: infinite;
}

/* Safari 4.0 - 8.0 */
@-webkit-keyframes c3 {
    0%   {background-color:red; left:800px; top:0px;}
    25%  {background-color:yellow; left:200px; top:-50px;}
    50%  {background-color:blue; left:600px; top:-200px;}
    75%  {background-color:green; left:100px; top:-400px;}
    100% {background-color:red; left:675px; top:-10px;}
}

/* Standard syntax */
